#include <iostream>
#include <string>

int main() {
    
    std::string firstName = "Isaias Daniel";
    std::string lastName = "Aleman Perez";
    std::string streetAddress = "1871 W 62nd Street";
    std::string city = "Hialeah";
    std::string zipCode = "33012";

 
    std::cout << "First Name: " << firstName << std::endl;
    std::cout << "Last Name: " << lastName << std::endl;
    std::cout << "Street Address: " << streetAddress << std::endl;
    std::cout << "City: " << city << std::endl;
    std::cout << "Zip Code: " << zipCode << std::endl;

    return 0;
}

